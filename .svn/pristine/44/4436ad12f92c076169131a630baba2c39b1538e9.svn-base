package com.activity.common.model.system.base;

import com.jfinal.plugin.activerecord.IBean;
import com.jfinal.plugin.activerecord.Model;

/**
 * 菜单与角色
 * 
 * @author 唐
 *
 */
@SuppressWarnings("serial")
public class BaseSystemMenuRole<M extends BaseSystemMenuRole<M>> extends Model<M> implements IBean {
	public void setId(java.lang.String ID) {
		set("ID", ID);
	}

	public java.lang.String getId() {
		return get("ID");
	}

	/**
	 * 菜单ID
	 * 
	 * @param ID
	 */
	public void setMenu(java.lang.String MENU) {
		set("MENU", MENU);
	}

	public java.lang.String getMenu() {
		return get("MENU");
	}

	/**
	 * 角色ID
	 * 
	 * @param ID
	 */
	public void setRole(java.lang.String ROLE) {
		set("ROLE", ROLE);
	}

	public java.lang.String getRole() {
		return get("ROLE");
	}
}
