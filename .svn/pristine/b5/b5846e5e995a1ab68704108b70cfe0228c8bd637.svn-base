.promo-ft .promo-opt a i {
	background: url(http://gtms01.alicdn.com/tps/i1/T1iG48FDpfXXX8QSDI-400-340.png);
	background-image: -webkit-image-set(url(http://gtms01.alicdn.com/tps/i1/T1iG48FDpfXXX8QSDI-400-340.png) 1x, url(http://gtms04.alicdn.com/tps/i4/T1LX06Fw8iXXcaXwo7-800-680.png) 2x);
	background-image: -moz-image-set(url(http://gtms01.alicdn.com/tps/i1/T1iG48FDpfXXX8QSDI-400-340.png) 1x, url(http://gtms04.alicdn.com/tps/i4/T1LX06Fw8iXXcaXwo7-800-680.png) 2x);
	background-image: -ms-image-set(url(http://gtms01.alicdn.com/tps/i1/T1iG48FDpfXXX8QSDI-400-340.png) 1x, url(http://gtms04.alicdn.com/tps/i4/T1LX06Fw8iXXcaXwo7-800-680.png) 2x);
	background-image: -o-image-set(url(http://gtms01.alicdn.com/tps/i1/T1iG48FDpfXXX8QSDI-400-340.png) 1x, url(http://gtms04.alicdn.com/tps/i4/T1LX06Fw8iXXcaXwo7-800-680.png) 2x);
	background-repeat: no-repeat;
}

html:root .promo-nav,
html:root .promo-opt .next,
html:root .promo-opt .next:hover,
html:root .promo-opt .prev,
html:root .promo-opt .prev:hover {
	filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr='#00000000', EndColorStr='#00000000')
}

.promo-ft .promo-opt {
	display: none;
}

.promo-ft .promo-opt a {
	display: block;
	height: 40px;
	line-height: 40px;
	overflow: hidden;
	*zoom: 1;
	position: absolute;
	top: 6.25vw;
	width: 40px;
	color: #fff;
	background-color: rgba(0, 0, 0, .3);
	filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr='#4C000000', EndColorStr='#4C000000')
}

.promo-ft .promo-opt a:hover {
	background-color: rgba(0, 0, 0, .6);
	filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr='#99000000', EndColorStr='#99000000')
}

.promo-ft .promo-opt a i {
	display: block;
	margin: 8px 0 0 12px;
	cursor: pointer;
	width: 15px;
	height: 23px;
	overflow: hidden;
	*zoom: 1;
}

.promo-ft .promo-opt .prev {
	left: 5px;
}

.promo-ft .promo-opt .prev i {
	background-position: -200px 0;
}

.promo-ft .promo-opt .next {
	right: 5px;
}

.promo-ft .promo-opt .next i {
	background-position: -200px -24px;
}

.promo-ft .promo-nav {
	position: absolute;
	bottom: 15px;
	left: 46%;
	height: 13px;
	text-align: center;
	font-size: 0;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	-ms-border-radius: 10px;
	-o-border-radius: 10px;
	border-radius: 10px;
	background: rgba(255, 255, 255, .3);
	filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, StartColorStr='#4CFFFFFF', EndColorStr='#4CFFFFFF')
}

.promo-ft .promo-nav li {
	display: -moz-inline-stack;
	display: inline-block;
	vertical-align: middle;
	*vertical-align: auto;
	zoom: 1;
	*display: inline;
	margin: 2px;
}

.promo-ft .promo-nav li a {
	display: block;
	padding-top: 9px;
	width: 9px;
	height: 0;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	-ms-border-radius: 50%;
	-o-border-radius: 50%;
	border-radius: 50%;
	background: #b7b7b7;
	overflow: hidden;
	*zoom: 1
}

.promo-ft .promo-nav .selected a {
	background: #f40
}
.mask {
	width: 100%;
	height: 100%;
	position: fixed;
	background: rgba(2,2,2,0.6);
	z-index: 998;
	left: 0;
	top: 0;
}

.popUp-window {
 	position:fixed;
    width:36.6vw;
    height:21.2vw;
    margin-left:-18.3vw;
    margin-top:-10.6vw;
    background:white;
    left:50%;
    right:0;
    top:50%;
    bottom:0;
    z-index:1000;
    box-shadow: 0px 0px 60px rgba(121,36,7,0.5);
    text-align: center;
	
}
.popUp-window .close{
	position: absolute;
	top:1.2vw;
	right: 1.2vw;
}
.popUp-window .infos{
	font-size:1.6vw;
	color:#1300000;
	margin-top:6.6vw;
}
.popUp-window input[type="button"]{
	width:13.2vw;
	height: 3.7vw;
	border:none;
	color:#ffffff;
	border-radius: 3px;
	font-size:1.8vw;
};
.popUp-window .btn-left{
	background: #A7A7A7;
	margin-top:4.4vw;
}
.popUp-window .btn-right{
	background: #F4A42E;
	margin-left: 5.1vw;
	margin-top:4.4vw;
}
