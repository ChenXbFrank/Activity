* {
	padding: 0px;
	margin: 0px;
}

html,
body {
	width: 100%;
	height: 100%;
}

body {
	font-family: "SimHei";
	height: 100%;
	position: relative;
}
.title1{
	width: 5.72rem;
	height:0.8rem;
	line-height:0.8rem;
	text-align:center;
	vertical-align:center;
	position: absolute;
	top:5%;
	left:50%;
	margin-left:-2.86rem;
	margin-top:-0.4rem;
}
.title1 img{
	max-width:100%;
	max-height:100%;
	vertical-align:middle;
}
.bg {
	width: 100%;
	height: 100%;
	background-size: 100% 100%;
	background-repeat: no-repeat;
	position: fixed;
	top: 0;
	left: 0;
	
}
.frist-main{
	width: 100%;
	display: -webkit-flex;
	display: flex;
	justify-content: center ;
	line-height:1.14666rem;
	margin-top:4rem;
}
.frist-main img{
	width: 1.146666rem;
	height: 1.146666rem;
	vertical-align: middle;
}
.frist-main p{
	color:white;
	font-size:0.42rem;
	margin-left:0.3rem;
}
.main-sceond{
	width: 100%;
	height:9.8rem;
	display: -webkit-flex;
	display: flex;
	justify-content: center ;
	flex-wrap: wrap;
	overflow-y:auto;
	
}
.duihuan_bg{
	width: 7.866666rem;
	height: 4.88rem;
	background-size: 7.866666rem 4.88rem;
	background-repeat: no-repeat;
	margin-top:0.5rem;
}
.product-pic {
	width: 41%;
	height: 3.5rem;
	z-index: 2;
	text-align:center;
	line-height:3.5rem;
	vertical-align:center;
	 margin-left:0.2rem;
	 margin-top:0.4rem;
	
}
.product-pic img{
	max-width:100%;
	max-height:100%;
	vertical-align:middle;
}
 .aaa{
	width:53%;
	height: 0.8rem;
	display: -webkit-flex;
	display: flex;
	justify-content: center ;
	text-align:center;
	line-height: 0.8rem;
	margin-top:-3.2rem;
	margin-left:3.55rem;

} 
 .aaa p{
	width: 4rem;
	height: 0.453333rem;
	font-size: 0.633333rem;
	color: #000000;
	font-family: "΢���ź�";
	font-size: 0.5rem;
	text-shadow: 0.006666rem 0.006666rem 0.006666rem #534204;
}
.duihuan_kuang{
	width: 3.853333rem;
	height: 0.8rem;
	background-size: 3.853333rem 0.8rem;
	background-repeat: no-repeat;
	text-align:center;
	line-height: 0.8rem;
	margin-left:3.73rem;
	margin-top:0.73rem;
	color:#C15C42;
}
.woyao_duihuan_kuang img{
	width: 3.733333rem;
	height: 0.693333rem;
	margin-left:3.75rem;
	margin-top:0.73rem;
}
.contain {
	width: 100%;
}

.contain .title {
	width: 8.306666rem;
	height: 2.266666rem;
	background-size: cover;
	background-repeat: no-repeat;
	margin: 0.3rem auto;
}
.contain ul {
	width: 100%;
	margin-top:1rem;
}
.contain ul li {
	width: 8.78rem;
	height: 9.12rem;
	margin: 0 auto;
	padding-bottom:1.06rem;
	position: relative;
	z-index: 1;
}

.contain ul li .exchangeInfos {
	width: 8.53rem;
	height: 100%;
}

.contain ul li .exchangeInfos .product-pc {
	width: 6.5rem;
	height: 7.77rem;
	text-align: center;
	margin: 0 auto;
	line-height:7.77rem;
	
}

.contain ul li .exchangeInfos .product-pc img {
	vertical-align:middle;
	max-height: 100%;
	max-width: 100%;
}

.contain ul li .exchangeInfos .font-infos {
	width: 100%;
	height: 2.34rem;
	margin: 0 auto;
	background: #F4F4F4;
	border-top: 1px solid #dadada;
	text-align: center;
}

.contain ul li .exchangeInfos .font-infos .product-name {
	margin: 0.4rem auto 0.2rem;
}

.contain ul li .exchangeInfos .font-infos .need-integral {
	width: 2.32rem;
	height: 0.48rem;
	border: 1px dashed #808080;
	background: #FFFFFF;
	color: #808080;
	line-height: 0.48rem;
	margin: 0 auto 0.14rem;
}

.contain ul li .exchangeInfos .font-infos del {
	color: #808080;
}



body .suspend {
	width:1rem;
	height: 1rem;
	background-size: contain;
	position: fixed;
	bottom: 0.34rem;
	right: 0.02rem;
	z-index: 1000;
	text-align: center;
	color: #808080;
	border-radius: 50%;
}
.scrollInfos{
	width:0.77rem;
	height: 0.77rem;
	line-height: 0.4rem;
	margin:0.2rem auto;
}
.topPic {
	line-height: 2.3rem;
	margin-top:-3rem;
}
body .suspend .topIcon {
	width: 0.56rem;
	height: 0.45rem;
	position:absolute;
	left:0.22rem;
	top:0.4rem;
}

body .suspend .line {
	height: 1px;
	width: 0.77rem;
	background: #808080;
	margin: 0 auto;
}

body .suspend .num {
	display: inline-block;
	font-size:0.52rem;
}
.screen{
	width:9rem;
	height: 100%;
	background-color: rgba(0, 0, 0, .9);
	position: fixed;
	margin-left: -10.166666rem;
	z-index: 10000;
	
}
.screen span,.section{
	color: white;
	font-size: 0.4rem;
	margin-left: 0.6rem;
	margin-top: 1rem;
}
.sort{
	color: white;
	font-size: 0.4rem;
	margin-left: 0.6rem;
	margin-top: 1rem;
}
.screen input{
	width: 2.5rem;
	height: 1rem;
	text-align: center;
	margin-left: 0.6rem;
	margin-top: 0.5rem;
}
.cancel{
	width: 46.5%;
	height: 1.5rem;
	background-color: #CCCCCC;
	display: inline-block;
	text-align: center;
	position:absolute;
	bottom:0;
	float: left;
	line-height: 1.5rem;
	color: white;
	font-size: 0.6rem;
}
.success{
	width: 50%;
	height: 1.5rem;
	background-color:#F58B25;
	display: inline-block;
	float: left;
	position:absolute;
	bottom:0;
	left:4.5rem;
	text-align: center;
	line-height: 1.5rem;
	color: #FDD507;
	font-size: 0.6rem;
}
.xian1{
	margin-top: 1rem;
	color: red;
}
.all{
	color: #F58B25;
	font-size: 0.4rem;
	margin-left: 0.6rem;
	margin-top: 0.2rem;
	border-bottom: 0.013333rem solid gainsboro;
}

.low{
	color: white;
	font-size: 0.4rem;
	margin-left: 0.6rem;
	margin-top: 0.5rem;
	border-bottom: 0.013333rem solid gainsboro;
}

.height{
	color: white;
	font-size: 0.4rem;
	margin-left: 0.6rem;
	margin-top: 0.5rem;
	border-bottom: 0.013333rem solid gainsboro;
}
body .shauixuan {
	width: 1rem;
    height: 1rem;
    background-size: contain;
    position: fixed;
    bottom: 0.34rem;
    top: 12rem;
    right: 0.02rem;
    z-index: 1000;
    text-align: center;
    color: #808080;
    border-radius: 50%;
}
body .shauixuan .downIcon {
	width: 0.56rem;
	height: 0.45rem;
	position:absolute;
	left:0.22rem;   
	top:0.3rem;
}
.downPic {
	line-height: 2.3rem;
}
.downPic img{
	width: 0.56rem;
	height: 0.48rem;
}
.ok{
	width: 0.493333rem;
	height: 0.333333rem;
    float: left;
    margin-left: 6rem;
    margin-top: -0.4rem;
}   
