* {
	padding: 0px;
	margin: 0px;
}

html,
body,
.container {
	width: 100%;
	height: 100%;
}

ul li {
	list-style: none;
}
img{
	border:none;
}
/* .clearfloat{
	width:180px;
	height:60px;
	
	
} */
.clearfloat:after {
	width: 0;
	height: 0;
	visibility: hidden;
	clear: both;
	display: block;
	content: "";
}

.container .nav {
	width: 16%;
	height: 100%;
	background-repeat: no-repeat;
	background-color:#832e10;
	background-size: cover;
	float: left;
	color: #f6f6f6;
	text-shadow: 0px 1px 2px rgba(154, 79, 18, 0.45);
	text-align: center;
	position: fixed;
}

.container .nav .head-portrait {
	/* width: 10vw;
	height: 10vw; */
	border: 8px solid #fff200;
	border-radius: 50%;
	margin: 5vw auto 2.2vw;
	text-align: center;
	line-height: 10vw;
}

.container .nav .head-portrait img {
	vertical-align: middle;
	/*width:100%;*/
	max-width: 100%;
	max-height: 100%;
}

.container .nav .name {
	font-size: 1.8vw;
	width: 100%;
	margin-top:5.9vw;
}

.container .nav .account-number {
	margin-top: 1.5vw;
	font-size: 1.2vw;
}

.container .nav ul {
	width: 100%;
	height:60vh;
	margin-top: 5vw;
	font-size: 1.2vw;
	overflow-y:auto;
}

.container .nav ul li {
	width: 100%;
	height: 4vw;
	/* line-height: 8.2vw; */
}

.add-bg {
	background: rgba(255, 241, 0, 0.4);
}

.bg-yellow {
	width: 0.4vw;
	height: 100%;
	display: inline-block;
	background: #fff100;
	float: left;
}

.container .nav ul li span,
.container .nav ul li img {
	vertical-align: middle;
	line-height:4vw;
}

.container .nav ul li img {
	margin-right: 1.6vw;
	width:2.1vw;
	height:2.1vw;
}

.container .nav a {
	display: inline-block;
	width: 100%;
	height: 100%;
	color: #f6f6f6;
	text-shadow: 0px 1px 2px rgba(154, 79, 18, 0.45);
	text-decoration: none;
}

.container .section {
	width: 84%;
	float: right;
	height:100%;
}

.container .section .title {
	width:84%;
	height: 8.3vw;
	background-repeat: no-repeat;
	background-color: #fbf4f1;
	background-size: cover;
	position:fixed;
}

.container .section .title .OA-name {
	font-size: 2.2vw;
	color: #f6f6f6;
	margin-left: 3.8vw;
	line-height: 7.3vw;
	float: left;
	text-shadow: 0px 2px 2px rgba(154, 79, 18, 0.45);
}

.container .section .title .exit{
		color:#407629;
		font-size:1.3vw;
		float:right;
		line-height:7.3vw;
		margin-right:1.7vw;
}

.container .section .title .exit span,
.container .section .title .exit img {
	vertical-align: middle;
}

.container .section .title .exit img {
	margin-left: 2.2vw;
}

#myframe {
	margin-top:8.1vw;
	width:100%;
	height:100%;
}
