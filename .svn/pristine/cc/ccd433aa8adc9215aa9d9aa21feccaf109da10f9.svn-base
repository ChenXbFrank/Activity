#bg-music{
	display:none;
	height: 0;
}
/*音符*/
.open{
    -webkit-animation: moveRo linear 3.5s  infinite;
    animation-delay:0;
}
.music{
	width: 1rem;
    height: 1rem;
}
.icon-music{
    width: 1rem;
    height: 1rem;
  	margin-top:1rem;
    background-size: 100% 100%;
    display: block;
}
.music-span{
    position: absolute;
    top: 0.5rem;
    left:0.4rem;
    width: 0.45rem;
    height: 0.45rem; 
    background-size: 100% 100%;
    -webkit-animation: openIconRo ease 1.2s both infinite;
    animation: openIconRo ease 1.2s both infinite;
}

@-webkit-keyframes moveRo {
    from {-webkit-transform: rotate(360deg);  }
    to { -webkit-transform: rotate(-360deg); }
}
@keyframes moveRo {
    from {  transform: rotate(-360deg)}
    to {  transform: rotate(360deg); }
}
/*小音符动画*/

@-webkit-keyframes openIconRo {
    0% { -webkit-transform: translateY(100%); opacity:0;}
    50% { -webkit-transform: translateY(0%); opacity:1;}
    100% { -webkit-transform: translateY(-100%); opacity:0;}
}
@keyframes openIconRo {
    0% { -webkit-transform: translateY(100%);
        transform: translateY(100%);
        opacity:0;
    }
    50% { -webkit-transform: translateY(0%); transform: translateY(0%); opacity:1;}
    100% { -webkit-transform: translateY(-100%); transform: translateY(-100%); opacity:0;}
}



@keyframes show_hide{
    from{
        opacity: 0;
    }to{
         opacity: 1;
     }
}
@-webkit-keyframes show_hide{
    from{
        opacity: 0;
    }to{
         opacity: 1;
     }
}
