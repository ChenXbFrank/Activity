* {
	padding: 0px;
	margin: 0px;
}

html,body{
	width: 100%;
	height: 100%;
}

.outer{
	width: 100%;
	height: 100%;
	background-color: #e04241;
	background-repeat: no-repeat;
	background-size: cover;
	overflow: hidden;
}

.clearfix:after {
	clear: both;
	content: "";
	width: 0;
	height: 0;
	visibility: hidden;
	display: block;
}

input[type="button"],
input[type="submit"],
input[type="reset"],
input[type="text"],
input[type="password"] {
	-webkit-appearance: none;
}
.rule{
	background:none;
	border:none;
	background-repeat: no-repeat;
	background-size:cover;
	width:1.82rem;
	height: 1.68rem;
	position: absolute;
	right:0.12rem;
	top:0.12rem;
}
.contain {
	margin-top: 1.2rem;
	overflow: hidden;
}

/*第二种*/
.index_nine{
	margin-top:0.8rem;
}
.index_nine .nine {
	width: 30%;
	height: 3.12rem;
	text-align: center;
	margin:0.5% 1% 0;
	list-style: none;
	display: inline-block;
	position:relative;
}
.index_nine .nine .wine {
	width: 0.82rem;
	height: 2.41rem;
	position: absolute;
	z-index: 1;
	left:50%;
	margin-left:-0.41rem;
	transform-origin:50% 50%;
}
.index_nine .nine .water {
	width: 1.92rem;
	height: 0.7rem;
	position: absolute;
	z-index: 2;
	left:50%;
	top:1.9rem;
	margin-left:-0.96rem;
}

.index_nine .nine .font_info {
	width: 1.86rem;
	height: 0.53rem;
	position: absolute;
	z-index: 3;
	left:50%;
	bottom:-0.1rem;
	margin-left:-0.93rem;
}
/*结束*/
.animation_class{
	animation:mymove 1s;
}
.btns {
	width: 100%;
	margin-top: 0.22rem;
}
/* .btns{
	width:100%;
	position: absolute;
	bottom:0.2rem;
} */
.btns button {
	background: none;
	border: none;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	width: 2.21rem;
	height: 0.82rem;
}

.btns .share {
	float: right;
	margin-right: 1.52rem;
}

.btns .my_award {
	float: left;
	margin-left: 1.52rem;
	
}


/*酒瓶抖动动效*/

@keyframes mymove {
	0% {
		/*transform: rotate(30deg);*/
		transform:scale(0.9,0.9);
	}
	10%{
		transform:scale(1,1);
		/*transform: rotate(-30deg);*/
	}
	20% {
		/*transform: rotate(10deg);*/
		transform:scale(1.3,1.3);
	}
	30% {
		transform: rotate(-30deg);
	}
	40% {
		transform: rotate(30deg);
	}
	50% {
		transform: rotate(-20deg);
	}
	60% {
		transform: rotate(20deg);
	}
	70% {
		transform: rotate(-10deg);
	}
	80% {
		transform: rotate(10deg);
	}
	90% {
		transform: rotate(-5deg);
	}
	95%{
		transform:scale(1.1,1.1);
	}
	100% {
		transform: rotate(0deg);
		transform:scale(1,1);
	}
}


