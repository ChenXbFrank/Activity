#table-outer{
	width:80vw;
}
#table-outer table{
	width:80vw;
}
#table-outer .table-tbody{
	width:80vw;
}
#table-outer table tr>td{
	width:10vw;
	height:6vw;
}
#table-outer table tr>td:nth-child(6){
	width:20vw;
}
#yulan{
	color:black;
	width:5vw;
	height:1.6vw;
}
.upPic{
	text-decoration:underline;
	color:red;
}
.upPic:hover,#yulan:hover{
	cursor:pointer;
}
.pic{
	margin:2px auto;
	max-width:100%;
	max-height:100%;
	vertical-align:middle;
}