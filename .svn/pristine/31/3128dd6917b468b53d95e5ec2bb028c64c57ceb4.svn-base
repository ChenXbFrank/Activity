.mask {
	width: 100%;
	height: 100%;
	position: fixed;
	background: rgba(2,2,2,0.6);
	z-index: 998;
	left: 0;
	top: 0;
}

.popUp-window {
 	position:fixed;
    width:36.6vw;
    height:21.2vw;
    margin-left:-18.3vw;
    margin-top:-10.6vw;
    background:white;
    left:50%;
    right:0;
    top:50%;
    bottom:0;
    z-index:1000;
    box-shadow: 0px 0px 60px rgba(121,36,7,0.5);
    text-align: center;
	
}
.popUp-window .close{
	position: absolute;
	top:1.2vw;
	right: 1.2vw;
}
.popUp-window .infos{
	font-size:1.6vw;
	color:#1300000;
	margin-top:6.6vw;
}
.popUp-window input[type="button"]{
	width:13.2vw;
	height: 3.7vw;
	border:none;
	color:#ffffff;
	border-radius: 3px;
	font-size:1.8vw;
};
.popUp-window .btn-left{
	background: #A7A7A7;
	margin-top:4.4vw;
}
.popUp-window .btn-right{
	background: #F4A42E;
	margin-left: 5.1vw;
	margin-top:4.4vw;
}
