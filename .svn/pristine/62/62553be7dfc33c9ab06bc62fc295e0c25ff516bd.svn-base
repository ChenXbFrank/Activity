package com.activity.common.model.system.base;
import com.jfinal.plugin.activerecord.IBean;
import com.jfinal.plugin.activerecord.Model;

/**
 * 图片上传
 * @author 唐
 *
 */
@SuppressWarnings("serial")
public class BaseUploadImg<M extends BaseUploadImg<M>> extends Model<M> implements IBean{
	public void setId(java.lang.String ID) {
		set("ID", ID);
	}

	public java.lang.String getId() {
		return get("ID");
	}
	
	/**
	 *图片地址
	 * 
	 * @param ID
	 */
	public void setImgUrl(java.lang.String IMG_URL) {
		set("IMG_URL", IMG_URL);
	}

	public java.lang.String getImgUrl() {
		return get("IMG_URL");
	}
	
	/**
	 * 用户code
	 * 
	 * @param ID
	 */
	public void setUserCode(java.lang.String USERCODE) {
		set("USERCODE", USERCODE);
	}

	public java.lang.String getUserCode() {
		return get("USERCODE");
	}
	
	/**
	 * 用户Id
	 * 
	 * @param ID
	 */
	public void setUserId(java.lang.String USERID) {
		set("USERID", USERID);
	}

	public java.lang.String getUserId() {
		return get("USERID");
	}
	
	/**
	 * 备注信息
	 * 
	 * @param ID
	 */
	public void setSpareOne(java.lang.String SPARE_ONE) {
		set("SPARE_ONE", SPARE_ONE);
	}

	public java.lang.String getSpareOne() {
		return get("SPARE_ONE");
	}
}
