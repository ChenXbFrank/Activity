* {
	padding: 0px;
	margin: 0px;
}

.main {
	width: 60vw;
}

.main .commonStyle {
	margin-top: 2vw;
}

.main .commonStyle label {
	font-size: 1vw;
	vertical-align: top;
}

.main .shareTitle {
	width: 15vw;
	height: 2vw;
	border-radius: 4px;
	border: 1px solid #d0d0d0;
	margin-left: 0.8vw;
}

.main textarea {
	border-radius: 4px;
}

.imgUp .pic-outer {
	display: inline-block;
	width: 10vw;
	height: 10vw;
	text-align: center;
	line-height: 10vw;
	vertical-align: middle;
	border: 1px solid #d0d0d0;
}

.imgUp .pic {
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
}

.imgUp #yulan {
	color: black;
	width: 5vw;
	height: 1.5vw;
}

.imgUp #zwb_upload {
	margin-left: 5vw;
}

.main-left {
	float: left;
}

.main .btns {
	width: 30vw;
	height: 3.3vw;
	margin: 2vw auto;
}

.main .btn {
	width: 13.17vw;
	height: 3.19vw;
}

.main .btns #save {
	float: left;
}

.main.btns #back {
	float: right;
}

.main .btn button {
	width: 100%;
	height: 100%;
	background-color: #eac85d;
	color: white;
	border-radius: 4px;
}

.imgUp {
	margin-top: 4vw;
	float: right;
	margin-right: 3vw;
}

.imgUp .picInfo {
	font-size: 0.8vw;
	color: red;
}

.sharePic {
	font-size: 1vw;
	float: left;
	margin-top: 4vw;
	text-align: center;
}

.pic-box {
	float: left;
	width: 12vw;
}

.main .upload {
	margin-left: 0vw;
}