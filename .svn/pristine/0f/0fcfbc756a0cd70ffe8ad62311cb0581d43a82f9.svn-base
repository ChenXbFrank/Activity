* {
	margin: 0;
	padding: 0;
}

input {
	border: none;
}

label {
	color: white;
}

body,
html {
	width: 100%;
	height: 100%;
	position: relative;
}
.main img{
	width: 3.053333rem;
	height: 2.466666rem;
	position: absolute;
	top: 40%;
	left: 50%;
	margin-left: -1.526666rem;
	margin-top: -1.233333rem;
}
.main .sorry{
	width: 3.9rem;
	height: 0.4rem;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -1.8rem;
	margin-top: -0.2rem;
	color: #376588;
	font-size: 0.346666rem;
}
.main .Jump span{
	width: 3.733333rem;
	height: 0.4rem;
	position: absolute;
	top: 65%;
	left: 50%;
	margin-left: -1.9rem;
	margin-top: -0.2rem;
	color: #ff4e1e;
	font-size: 0.266666rem;
}
.main .Jump p{
	width: 3.933333rem;
	height: 0.4rem;
	position: absolute;
	top: 65%;
	left: 50%;
	margin-left: -1.566666rem;
	margin-top: -0.2rem;
	color: #292929;
	font-size: 0.266666rem;
}
.personal{
	width: 4rem;
	height: 1.066666rem;
	background-color: #FCB213;
	color: white;
	text-align: center;
	line-height: 1.066666rem;
	border-radius: 0.133333rem;
	position: absolute;
	top: 80%;
	left: 50%;
	margin-left: -2rem;
	margin-top: -0.533333rem;
	font-size: 0.32rem;
}
