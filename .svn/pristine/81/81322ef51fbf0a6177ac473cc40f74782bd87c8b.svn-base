}

.upload input[type="button"] {
	width: 8vw;
	height: 2vw;
	line-height: 2vw;
	font-size: 1vw;
	border-radius: 4px;
	border: none;
	background: #f19032;
	color: white;
}

.upload1 input[type="button"] {
	width: 8vw;
	height: 2vw;
	line-height: 2vw;
	font-size: 1vw;
	border-radius: 4px;
	border: none;
	background: #f19032;
	color: white;
}


