.upfilebox {
	width: 20.5vw;
	position: relative;
	margin: 10px auto 0px;
	margin-left: 4.1vw;
	text-align: center;
}

.upfilebox input[type="file"] {
	width: 6.2vw;
	height: 2vw;
	position: relative;
	z-index: 9;
	opacity: 0;
}

.upfilebox label {
	position: absolute;
	background: #f19032;
	display: inline-block;
	color: #fff;
	width: 6.2vw;
	height: 2vw;
	line-height: 2vw;
	text-align: center;
	top: 0px;
	left: 50%;
	margin-left: -3.1vw;
	border-radius: 4px;
	font-size: 16px;
}

.upload {
	margin-left: 4.1vw;
}

.left .picInfo1 {
	font-size: 1vw;
	margin-left: 4.1vw;
	color: red;
	margin-top: 1vw;
}

.chazhao,
.chazhao2 {
	width: 5vw;
	height: 2vw;
	border-radius: 4px;
	background: #F4A42E;
	border: none;
	color: white;
	font-size: 1vw;
}

.qwe {
	margin-top: -0.6vw;
}

.qwe1 {
	margin-bottom: 0.6vw;
	margin-top: -1vw;
	font-size: 1vw;
}

.parting-line {
	width: 100%;
	height: 1px;
	background: #a5b2ba;
	margin-top: 1vw;
}

.content {
	color: red;
}


/* 下拉框 */

#bargaining-situations {
	width: 10vw;
	height: 2vw;
	margin-top: 1vw;
	border-color: #d0d0d0;
	background-repeat: no-repeat;
	background-position: right center;
	background-size: contain;
}

.select {
	width: 10vw;
	border: 1px solid #d0d0d0;
	margin: 0;
	padding: 0;
	position: absolute;
	right: 0;
	border-radius: 4px;
	background: white;
}

.select li {
	list-style: none;
	width: 100%;
	height: 2.2vw;
	text-align: left;
	line-height: 2.2vw;
}

.bargain-status {
	position: relative;
}

.smallImg {
	position: absolute;
	right: 0.6vw;
	top: 40%;
}

.outer {
	position: relative;
	display: inline-block;
}

textarea {
	width: 20vw;
	font-size: 0.8vw;
	vertical-align: top;
}


/* 弹窗遮罩 */

.fade {
	width: 100%;
	height: 100%;
	background: rgba(2, 2, 2, 0.6);
	position: fixed;
	left: 0;
	top: 0;
	z-index: 9000;
}


/*弹窗*/

.delivery-success {
	text-align: center;
	width: 20vw;
	height: 8vw;
	background: rgba(255, 255, 255, 1);
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left: -10vw;
	margin-top: -4vw;
	z-index: 9999;
	font-size: 1vw;
}

.success-outer {
	width: 18vw;
	height: 6vw;
	margin: 1vw auto 0px;
}

.delivery-success input[type="button"] {
	width: 5vw;
	height: 2vw;
	margin-top: 2vw;
	background: #fcb213;
	border: none;
	color: white;
	font-size: 1vw;
	border-radius: 4px;
}


/* 图片库 */

