a,
button,
input {
	outline: none;
}

#awardPop input[type="text"],
#billPop input[type="text"] {
	width: 10vw;
	height: 2vw;
	border-color: #d0d0d0;
}
#awardPop input[type="text"]{
	margin-top:1vw;
}
#billPop .skip input[type="text"] {
    width: 4vw;
