}

.imgsBox ul li img {
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
}
/*删除图片库中的一个图片*/
.deleteDiv{
	width:1vw;
	height:1.2vw;
	margin:0 auto;
	background-repeat:no-repeat;
