	width: 12vw;
	height: 12vw;
	line-height: 12vw;
	vertical-align: middle;
	text-align: left;
}

.addPic-upload img {
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
}

.pic-box .picInfo1 {
	color: red;
	font-size: 1vw;
