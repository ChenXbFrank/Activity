.imgsBox {
	width: 50vw;
	height: 18vw;
	margin: 1vw auto;
	border: 1px solid gray;
	overflow: auto;
}

.imgsBox ul li {
	list-style: none;
	width: 10vw;
	height: 10vw;
	float: left;
	line-height: 10vw;
	text-align: center;
	/* border:1px solid yellow; */
