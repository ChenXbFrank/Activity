.container p:nth-child(1){
	margin-top:2vw;
	font-weight:bold;
}
#cell_phone_number {
    width: 10vw;
    height: 2vw;
    margin-top: 1vw;
    border-color: #d0d0d0;
    background-repeat: no-repeat;
    background-position: right center;
    background-size: contain;
 
  
}
#password {
    width: 16vw;
    height: 2vw;   
    margin-top: 1vw;
    border-color: #d0d0d0;
    background-repeat: no-repeat;
    background-position: right center;  
    background-size: contain;
}
#address_{
	 width: 31vw;
    height: 2vw;   
    margin-top: 1vw;
    border-color: #d0d0d0;
    background-repeat: no-repeat;
    background-position: right center;
    background-size: contain;
}
#table-outer table tr>td:nth-child(5) {
    width: 8vw;
}
.prompt {
	text-align: center;
	width: 20vw;
	height: 8vw;
	background: rgba(255, 255, 255, 1);
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left: -10vw;
	margin-top: -4vw;
	z-index: 9999;
	font-size: 1vw;
}

.prompt p {
	margin-top: 1.6vw;
}
.prompt .yes {
	width: 5vw;
	height: 2vw;
	margin-top: 2vw;
	background: #F19032;
	border: none;
	color: white;
	font-size: 1vw;
	border-radius: 4px;
}
.prompt1 {
	text-align: center;
	width: 20vw;
