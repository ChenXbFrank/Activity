						var qrcode = $("#qrcode").val();
						var groupPurchaseId = $("#groupPurchaseId").val();   
						var groupPurchaseWxUserId = $("#groupPurchaseWxUserId").val();
						$.ajax({
							type: "GET",
							url: "${basePath}/index/WxConfig",
							async: true,
							dataType: "json",
							data: {
								"locationUrl": location.href
							},
							success: function(config) {
								wx.config({
