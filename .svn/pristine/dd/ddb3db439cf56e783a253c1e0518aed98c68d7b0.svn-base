														nonceStr: data.nonceStr, // 支付签名随机串，不长于 32 位
														package: data.prepay_id, // 统一支付接口返回的prepay_id参数值，提交格式如：prepay_id=***）
														signType: data.signType, // 签名方式，默认为'SHA1'，使用新版支付需传入'MD5'
														paySign: data.paySign, // 支付签名
														success: function(res) {
															// 支付成功后的回调函数
															// 使用以上方式判断前端返回,微信团队郑重提示：res.err_msg将在用户支付成功后返回    ok，但并不保证它绝对可靠。
															//alert("支付成功-等待收货-请到公众号菜单个人中心查看运单号");

															$(".already").show();
															$(".join").hide();
															$(".full").hide();
															$(".zhifu").hide();
															$("#ok").show();
															$(".text0").show();
															location.reload()
															location.href = "${basePath}/groupPurchase/successJoin?groupPurchaseWxUserId=" + groupPurchaseWxUserId + "&openid=" + openid + "&sellerNumberId=" + sellerNumberId + "&qrcode=" + qrcode;

														}
													});
												} else {
													alert("参团失败，该团已满！");
													location.reload()
												}
											}
								
										}
									});
								});
							}
						});
					});
				}
			} else { //未参加
				$(".already").hide();
				$(".join-hui").hide();
				$(".zhifu").hide();
				$(".full").show();
				$(".join").hide();
				$(".renshu").html('');
			}
		}
		/*点击加入好友*/
		$(".join").click(function() {
			
			var sellerNumberId = $("#sellerNumberId").val();
			var openid = $("#openid").val();
			var qrcode = $("#qrcode").val();
			var groupPurchaseId = $("#groupPurchaseId").val();
			var groupPurchaseWxUserId = $("#groupPurchaseWxUserId").val();
			$.ajax({
				type: "GET",
				url: "${basePath}/index/WxConfig",
				async: true,
				dataType: "json",
				data: {
					"locationUrl": location.href
				},
				success: function(config) {
					wx.config({
						debug: false, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印。
						appId: config.appId, // 必填，公众号的唯一标识
						timestamp: config.timestamp, // 必填，生成签名的时间戳
						nonceStr: config.nonceStr, // 必填，生成签名的随机串
						signature: config.signature, // 必填，签名，见附录1
						jsApiList: ['chooseWXPay'] // 必填，需要使用的JS接口列表，所有JS接口列表见附录2
					});
					wx.ready(function() {
						/* alert('${out_trade_no}'); */
						$.ajax({
							type: "GET",
							url: "${basePath}/groupPurchase/joinGroupPay",
							async: false,
							dataType: "json",
							data: {
								"groupPurchaseWxUserId": groupPurchaseWxUserId,
								"sellerNumberId": sellerNumberId,
								"groupPurchaseId": groupPurchaseId,
								"openid": openid,
								"out_trade_no":'${out_trade_no}'
							},
							success: function(data) {
								if(data.result == "nofollow"){
									$("#end").show();
								}else{
									if(data.result == "true") {
										wx.chooseWXPay({
											timestamp: data.timestamp, // 支付签名时间戳，注意微信jssdk中的所有使用timestamp字段均为小写。但最新版的支付后台生成签名使用的timeStamp字段名需大写其中的S字符
											nonceStr: data.nonceStr, // 支付签名随机串，不长于 32 位
											package: data.prepay_id, // 统一支付接口返回的prepay_id参数值，提交格式如：prepay_id=***）
											signType: data.signType, // 签名方式，默认为'SHA1'，使用新版支付需传入'MD5'
											paySign: data.paySign, // 支付签名
											success: function(res) {
												// 支付成功后的回调函数
												// 使用以上方式判断前端返回,微信团队郑重提示：res.err_msg将在用户支付成功后返回    ok，但并不保证它绝对可靠。
												//alert("支付成功-等待收货-请到公众号菜单个人中心查看运单号");
												$(".already").show();
												$("#ok").show();
												$(".text0").show();
												$(".join").hide();
												location.reload()
												location.href = "${basePath}/groupPurchase/successJoin?groupPurchaseWxUserId=" + groupPurchaseWxUserId + "&openid=" + openid + "&sellerNumberId=" + sellerNumberId + "&qrcode=" + qrcode;
											}
										});
									} else {
										alert("参团失败，该团已满！");
										location.reload()
									}
								}
				
							}
						});
					});
				}
			});
		})
		/*关闭分享*/
		$(".share").click(function() {
			$(".share").hide();
		})
		/*关闭恭喜页面*/
		$(".close").click(function() {
			$(".success,.fail").hide();
		})
		$(".close1").click(function() {
			$(".Rule").hide();
		})
		/*点击活动规则*/
		$(".rule-name").click(function() {
			$(".Rule").show();
		})

		$(".person,.person-center").click(function() {
			var sellerNumberId = $("#sellerNumberId").val();
			location.href = "${basePath}/personalCenter/index?BPN=" + sellerNumberId;
		})
	</script>

</html>